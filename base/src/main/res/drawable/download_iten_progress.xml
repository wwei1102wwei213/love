<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp" />
            <gradient
                android:angle="0"
                android:centerColor="#434346"
                android:centerY="0.75"
                android:endColor="#434346"
                android:startColor="#434346" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp" />
                <gradient
                    android:angle="0"
                    android:centerColor="#E3B88E"
                    android:centerY="0.75"
                    android:endColor="#E3B88E"
                    android:startColor="#E3B88E" />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp" />
                <gradient
                    android:angle="0"
                    android:endColor="#E3B88E"
                    android:startColor="#E3B88E" />
            </shape>
        </clip>
    </item>
</layer-list>