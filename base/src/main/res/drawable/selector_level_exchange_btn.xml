<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 设置背景色 -->
    <item android:id="@android:id/background" android:state_selected="true">
        <shape>
            <corners android:radius="25dp" />
            <solid android:color="#E3B88E"/>
        </shape>
    </item>
    <!-- 设置背景色 -->
    <item android:id="@android:id/background" android:state_checked="true">
        <shape>
            <corners android:radius="25dp" />
            <solid android:color="#E3B88E"/>
        </shape>
    </item>
    <!-- 设置背景色 -->
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="25dp"/>
            <solid android:color="#434346"/>
        </shape>
    </item>
</selector>